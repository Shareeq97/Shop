<h4><strong>Assigned Work</strong></h4>
<% features.each do |feature| %>
  <div class="panel-feature-list panel-default">
    <div class="panel-head">
      <h4 class="panel-title">
        <button class="btn" type="button" data-toggle="collapse" data-target="#<%= feature.feature_name.gsub(/\s+/, '') %>"> <%= feature.feature_name %>
        </button>
      </h4>
    </div>
    <div id="<%= feature.feature_name.gsub(/\s+/, '') %>" class="panel-collapse collapse">
      <div class="panel-body">
        <div>
          <%= render partial: "features/show_assigned_feature" , locals: { feature: feature } %>
        </div>
      </div>
    </div>
  </div>
<% end %>

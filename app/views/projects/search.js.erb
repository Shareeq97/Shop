var search_div = $("<div id='search-box'>").html("<%= escape_javascript(render partial: 'projects/search_panel') %>");
$('#search-box').remove();
$("#project-panel-parent").prepend(search_div);
$('#idCI, #idIB, #idBL, #search-box').css("width",'24.76%');

function reset_panels(panel) {
	panel.parentElement.style.display='none';
  $('#idCI, #idIB, #idBL').css("width",'33.11666666666667%');
}
